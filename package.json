  },
  "homepage": "https://github.com/kiranparajuli589/express_sequelize/#readme",
  "dependencies": {
    "date-fns": "^2.29.3",
    "express": "^4.18.1",
    "sequelize": "6.28.1",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.1.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "ts-loader": "^9.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
