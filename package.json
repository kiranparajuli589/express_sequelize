{
  "name": "express_sequelize",
  "version": "0.0.1",
  "description": "A demonstration with Express & Sequelize",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "tsc && sequelize-cli db:migrate",
